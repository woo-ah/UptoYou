<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.uptoyou_game3.mapper.GameMapper">

	<select id="listAll" resultType="com.example.uptoyou_game3.dto.GameDto$AwardResDto">
		SELECT
			username,
			avg_time AS time
		FROM user
		WHERE avg_time IS NOT NULL
		ORDER BY avg_time ASC
	</select>

</mapper>